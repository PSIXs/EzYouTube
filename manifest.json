{
  "manifest_version": 2,

  "name": "YoutubeAgeBypass",
  "description": "This extension bypass youtube age restriction",
  "version": "0.9.0",

  "page_action": {
    "default_popup": "popup.html",
    "default_title": "YoutubeAgeBypass"
  },

  "background": {
    "scripts": ["eventPage.js"],
    "persistent": false
  },

  "content_scripts":[
    {
      "matches": ["https://*/*"],
      "js": ["content.js", "jquery-3.1.0.min.js"]
    }
  ],

  "permissions": ["tabs", "storage"]
}